# Nest
PORT=3100
NEST_ENVIRONMENT=production

# Security
JWT_ACCESS_PRIVATE_KEY='your-jwt-access-private-key-here'
JWT_ACCESS_PUBLIC_KEY='your-jwt-access-public-key-here'
JWT_REFRESH_PRIVATE_KEY='your-jwt-refresh-private-key-here'
JWT_REFRESH_PUBLIC_KEY='your-jwt-refresh-public-key-here'
CRYPTO_ALGORITHM="aes-256-cbc"
CRYPTO_SECRET_KEY="your-crypto-secret-key-32-chars"
CRYPTO_IV="your-crypto-iv-16-chars"
JWT_ACCESS_EXPIRES_IN=1d
JWT_REFRESH_EXPIRES_IN=7d

# Database - Neon PostgreSQL
# 형식: postgresql://[user]:[password]@[host]/[dbname]?sslmode=require
DATABASE_URL="postgresql://username:password@ep-xxx.us-east-2.aws.neon.tech/neondb?sslmode=require"

# Stock API
STOCK_SEARCH_API_URL=https://wts-info-api.tossinvest.com/api/v3/search-all/wts-auto-complete
STOCK_INFO_API_URL=https://wts-info-api.tossinvest.com/api/v2/stock-infos?codes=
STOCK_PRICE_API_URL=https://wts-info-api.tossinvest.com/api/v2/stock-prices?codes=

# Coin API
COIN_PRICE_API_URL=https://pro-api.coinmarketcap.com/v2/cryptocurrency/quotes/latest

# Exchange API
EXCHANGE_RATE_API_URL=https://openexchangerates.org/api/latest.json

# Redis - Upstash Redis
# Upstash에서 Redis 정보를 복사하세요:
# - Endpoint: [endpoint].upstash.io
# - Port: 6379 (TLS) 또는 6380
# - Password: Upstash 콘솔에서 확인
REDIS_HOST=your-endpoint.upstash.io
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_TLS=true
