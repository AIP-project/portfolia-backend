  "scripts": {
    "========== Local ==========": "",
    "docker:compose": "ts-node --project ./tsconfig.json ./deploy/docker-compose.ts",
    "docker:build": "ts-node --project ./tsconfig.json ./deploy/image-build.ts",
    "========== Deploy ==========": "",
    "docker:deploy": "ts-node --project ./tsconfig.json ./deploy/image-deploy.ts",
    "service:deploy": "ts-node --project ./tsconfig.json deploy/service-deploy.ts",
    "========== ETC ==========": "",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "db-sync-staging": "./script/db-replication.sh staging",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
